! new page
size 19 12

set hei .5 lwidth .06

set font psh
begin translate -1 0
begin graph
size   21 13

data field.dat
!data field_0.dat

nobox
!xaxis min 0 max 20
yaxis min 0 max 20
!yaxis min .01 max 150 log
xlabels hei  .6
ylabels hei  .6
y2axis on nticks 4
y2labels
y2axis 
xticks
yticks 
title "   " hei 0.6 font rmi
ytitle "     Field strength        "
xtitle "      k_0x     "
